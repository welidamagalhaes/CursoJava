package poo;

public class Caneta03 {
          String modelo;
          String cor;
          float ponta; 
          float carga;
          
          
          public void escrever () {
		}
          public void tampar () {
          }
		public Caneta03(String modelo, String cor, float ponta, float carga) {
			super();
			this.modelo = modelo;
			this.cor = cor;
			this.ponta = ponta;
			this.carga = carga;
